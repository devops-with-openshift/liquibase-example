<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="${author}" id="DEMO-DBMS-OPTION" dbms="mssql">
        <comment>Simple changeset to show DBMS option in liquibase</comment>
        <createTable tableName="SQLSERVER_ONLY">
            <column name="id" type="CHAR(2)">
                <constraints nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(24)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
